<div class="card">
  <div class="card-body">
  <h2>My Bookings</h2>
  <% @bookings.each do |booking| %>
    <strong><%= booking.car.title %></strong> <%= link_to "View booking", booking_path(booking) %>
    <p>Booked for: <%= booking.start_date %> - <%= booking.end_date %> </p>
  <% end %>
  </div>
</div>
